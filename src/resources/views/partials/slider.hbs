<div id="" class="slider  d-flex jc-center">
    <div class="container  d-flex jc-center ai-center">
        {{!-- <div class="content w-80"> --}}

            <div class="search-content">
                <div class="search d-flex">
                    <div class=" search-classer  ai-center  category">
                        <i class="fa-solid fa-house"></i>
                        <select class="filter-item btn pd--8-12">
                            <option value="">Loại nhà đất </option>
                            {{#each chuyenmucData}}

                            <option value="{{this.ma_so}}">{{this.ten_chuyen_muc}}</option>
                            {{/each}}
                        </select>
                    </div>
                    <div class="search-box ">
                        <input type="text" placeholder="Tìm nhanh . VD : Vinhomes Cental Park ..." class="pd--8-12"
                               name="seachInput">
                        <button class="btn-search btn ">
                            <i class="fa-solid fa-magnifying-glass"></i>
                            <span>Tìm kiếm</span>
                        </button>
                    </div>
                </div>
                <div class="filter d-flex jc-between mt-16">
                    <div class="col-95 d-flex">
                        <div class="col-25 district">
                            <select class="filter-item btn pd--8-12">
                                <option value="">Quận/Huyện </option>
                                {{#each diaLyData}}

                                <option value="{{this.ma_so}}">Quân {{this.ten_quan_huyen}}</option>
                                {{/each}}
                            </select>
                        </div>
                        <div class="col-25 price">
                            <select class="filter-item btn pd--8-12">
                                <option value="">Mức giá </option>
                                <option data-max="1">Dưới 1 triệu</option>
                                <option data-min="1" data-max="3">1- 3 triệu</option>
                                <option data-min="3" data-max="5">3- 5 triệu</option>
                                <option data-min="5" data-max="10">5- 10 triệu</option>
                                <option data-min="10" data-max="15">10- 15 triệu</option>
                                <option data-min="1">Trên 15 triệu</option>
                            </select>
                        </div>
                        <div class="col-25 area">
                            <select class="filter-item btn pd--8-12">
                                <option value="">Diện tích </option>
                                <option data-max="20">Dưới 20 m<sub>2</sub></option>
                                <option data-min="20" data-max="30">Từ 20 - 30 m<sub>2</sub></option>
                                <option data-min="30" data-max="50">Từ 30 - 50 m<sub>2</sub></option>
                                <option data-min="50" data-max="70">Từ 50 - 70 m<sub>2</sub></option>
                                <option data-min="70" data-max="100">Từ 70 - 100 m<sub>2</sub></option>
                                <option data-min="100">Trên 100 m<sub>2</sub></option>
                            </select>
                        </div>
                        <div class="col-25 more">
                            <select class="filter-item btn pd--8-12">
                                <option value="">Lọc thêm </option>

                            </select>
                        </div>
                    </div>
                    <div class="pd--8-12 btn col-5 btn-refresh">
                        <i class="fa-solid fa-arrows-rotate"></i>
                        <span></span>
                    </div>
                </div>
            </div>
            {{!--
        </div> --}}
    </div>

</div>